[
  {
    "route" : "([0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}).ip.{domain}",
    "type" : "A",
    "response" : "\\1"
  },
  {
    "route" : "simple\\.cname\\.{domain}",
    "record" : "CNAME",
    "response" : "www.{domain}"
  },
  {
    "route" : "loop\\.cname\\.{domain}",
    "record" : "CNAME",
    "response" : "loop.cname.{domain}"
  },
  {
    "route" : "(.*\\.)?{domain}",
    "type" : "A",
    "response" : "{ipv4}"
  },
  {
    "route" : "(.*\\.)?{domain}",
    "type" : "AAAA",
    "response" : "{ipv6}"
  }
]
